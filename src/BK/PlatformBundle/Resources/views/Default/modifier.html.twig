{% extends "BKPlatformBundle::accueil.html.twig" %}


	
{% block body %}

{% stylesheets 'bundles/bkplatform/css/main.css' %}
		<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
{% endstylesheets %}

<div class="well">
   {{ form_start(form) }}

    <div class="form-group">
		{{ form_label(form.login, "Login") }}
		<div class="col-sm-4">
        {{ form_widget(form.login) }}
		</div>
    </div>
	
	<div class="form-group">
		{{ form_label(form.password, "Mot de Passe") }}
		<div class="col-sm-4">
        {{ form_widget(form.password) }}
		</div>
    </div>
	
	<div class="form-group">
		{{ form_label(form.age, "Age") }}
		<div class="col-sm-4">
        {{ form_widget(form.age) }}
		</div>
    </div>
	
	<div class="form-group">
		{{ form_label(form.famille, "Famille") }}
		<div class="col-sm-4">
        {{ form_widget(form.famille) }}
		</div>
    </div>
	
	<div class="form-group">
		{{ form_label(form.race, "Race") }}
		<div class="col-sm-4">
        {{ form_widget(form.race) }}
		</div>
    </div>
	
	<div class="form-group">
		{{ form_label(form.nourriture, "Nourriture") }}
		<div class="col-sm-4">
        {{ form_widget(form.nourriture) }}
		</div>
    </div>
	
	{{ form_widget(form.Annuler) }}
	
	{{ form_widget(form.Modifier) }}
	
	{{ form_end(form) }}
	
</div>

{% endblock %}	  